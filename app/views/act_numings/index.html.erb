<p id="notice"><%= notice %></p>

<h1>Act Numings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @act_numings.each do |act_numing| %>
      <tr>
        <td><%= link_to 'Show', act_numing %></td>
        <td><%= link_to 'Edit', edit_act_numing_path(act_numing) %></td>
        <td><%= link_to 'Destroy', act_numing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Act Numing', new_act_numing_path %>
