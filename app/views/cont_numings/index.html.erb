<p id="notice"><%= notice %></p>

<h1>Cont Numings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cont_numings.each do |cont_numing| %>
      <tr>
        <td><%= link_to 'Show', cont_numing %></td>
        <td><%= link_to 'Edit', edit_cont_numing_path(cont_numing) %></td>
        <td><%= link_to 'Destroy', cont_numing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

