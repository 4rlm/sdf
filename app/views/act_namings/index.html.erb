<p id="notice"><%= notice %></p>

<h1>Act Namings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @act_namings.each do |act_naming| %>
      <tr>
        <td><%= link_to 'Show', act_naming %></td>
        <td><%= link_to 'Edit', edit_act_naming_path(act_naming) %></td>
        <td><%= link_to 'Destroy', act_naming, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Act Naming', new_act_naming_path %>
