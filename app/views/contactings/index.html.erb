<p id="notice"><%= notice %></p>

<h1>Contactings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactings.each do |contacting| %>
      <tr>
        <td><%= link_to 'Show', contacting %></td>
        <td><%= link_to 'Edit', edit_contacting_path(contacting) %></td>
        <td><%= link_to 'Destroy', contacting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contacting', new_contacting_path %>
