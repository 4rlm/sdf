<p id="notice"><%= notice %></p>

<h1>Urlings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urlings.each do |urling| %>
      <tr>
        <td><%= link_to 'Show', urling %></td>
        <td><%= link_to 'Edit', edit_urling_path(urling) %></td>
        <td><%= link_to 'Destroy', urling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Urling', new_urling_path %>
